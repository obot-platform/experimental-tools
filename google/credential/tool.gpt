Name: Google OAuth Credential
Share Credential: google-cred as google-search-console
Type: credential

---
Name: google-cred
Tools: github.com/obot-platform/tools/oauth2

#!sys.call github.com/obot-platform/tools/oauth2

{
    "oauthInfo": {
        "integration": "google-search-console",
        "token": "GOOGLE_SEARCH_OAUTH_TOKEN",
        "scope": [
            "https://www.googleapis.com/auth/userinfo.email",
            "openid",
            "https://www.googleapis.com/auth/userinfo.profile",
            "https://www.googleapis.com/auth/spreadsheets",
            "https://www.googleapis.com/auth/documents",
            "https://mail.google.com/",
            "https://www.googleapis.com/auth/gmail.readonly",
            "https://www.googleapis.com/auth/gmail.compose",
            "https://www.googleapis.com/auth/webmasters",
            "https://www.googleapis.com/auth/webmasters.readonly",
            "https://www.googleapis.com/auth/drive"
        ]
    }
}
